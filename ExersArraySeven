package Arrays;
/*7 Посчитать количество элементов больше нуля*/
/**
 * Created by vika on 01.08.15.
 */
public class ExersArraySeven {
    public static void main(String[] args) {
        int arrRed[] = { 1, 5, 9, 0, 6, -25, 85, -36, 75, -85};
        int count = 0;
        int example = 0;
        for(int i = 0; i < arrRed.length; i++){
            if(arrRed[i] > example ){
                count++;
            }
        }
        System.out.println(count);
    }
}
